RewriteEngine on

RewriteRule ^([0-9]+)/yardocs$                      http://%{HTTP_HOST}/builds/4x/$1/yardocs/ [R,L]
RewriteRule ^([0-9]+)/yardocs/$                     https://projectodd.ci.cloudbees.com/job/torquebox-4/$1/artifact/doc/index.html [P]

RewriteRule ^(LATEST/)?yardocs$                     http://%{HTTP_HOST}/builds/4x/$1yardocs/ [R,L]
RewriteRule ^(LATEST/)?yardocs/$                    https://projectodd.ci.cloudbees.com/job/torquebox-4/lastSuccessfulBuild/artifact/doc/index.html [P]
